				{text:"100℃","subtext":"温度","datatext":"温度","lastcolor":"#f0b43d","focuscolor":"#8b9398"},
				{text:"100kw/h","subtext":"电能量","datatext":"电能量","lastcolor":"#f0b43d","focuscolor":"#8b9398"},
				{text:"100kw/h","subtext":"消耗有功电度","datatext":"消耗有功电度","lastcolor":"#f0b43d","focuscolor":"#8b9398"},
				{text:"100kw/h","subtext":"发出有功电度","datatext":"发出有功电度","lastcolor":"#f0b43d","focuscolor":"#8b9398"},
				{text:"100kw/h","subtext":"绝对值和有...","datatext":"绝对值和有功电度","lastcolor":"#f0b43d","focuscolor":"#8b9398"},
				{text:"100kw/h","subtext":"净有功电度","datatext":"净有功电度","lastcolor":"#099575","focuscolor":"#8b9398"},
				{text:"100kw/h","subtext":"感性无功电度","datatext":"感性无功电度","lastcolor":"#099575","focuscolor":"#8b9398"},
				{text:"100kw/h","subtext":"容性无功电度","datatext":"容性无功电度","lastcolor":"#099575","focuscolor":"#8b9398"},
				{text:"100kw/h","subtext":"绝对值和无...","datatext":"绝对值和无功电度","lastcolor":"#099575","focuscolor":"#8b9398"},
				{text:"100kw/h","subtext":"净无功电度","datatext":"净无功电度","lastcolor":"#099575","focuscolor":"#8b9398"}
			]
			var erightdata=[
				{text:"正常","subtext":"智能终端状态","datatext":"智能终端状态","lastcolor":"#e05f66","focuscolor":"#8b9398"},
				{text:"正常","subtext":"通信状态","datatext":"通信状态","lastcolor":"#e05f66","focuscolor":"#8b9398"},
				{text:"正常","subtext":"输入开关位置","datatext":"输入开关位置","lastcolor":"#e05f66","focuscolor":"#8b9398"},
				{text:"正常","subtext":"输出开关位置","datatext":"输出开关位置","lastcolor":"#e05f66","focuscolor":"#8b9398"},
				{text:"正常","subtext":"开关储能机构","datatext":"开关储能机构","lastcolor":"#e05f66","focuscolor":"#8b9398"},
				{text:"正常","subtext":"操作电源","datatext":"操作电源","lastcolor":"#f0b43d","focuscolor":"#8b9398"},
				{text:"正常","subtext":"SF6气体密度...","datatext":"SF6气体密度及开关压力","lastcolor":"#f0b43d","focuscolor":"#8b9398"},
				{text:"正常","subtext":"相位检测","datatext":"相位检测","lastcolor":"#f0b43d","focuscolor":"#8b9398"},
				{text:"正常","subtext":"过电压检测","datatext":"过电压检测","lastcolor":"#f0b43d","focuscolor":"#8b9398"},
				{text:"正常","subtext":"短路检测","datatext":"短路检测","lastcolor":"#f0b43d","focuscolor":"#8b9398"},
				{text:"正常","subtext":"断路检测","datatext":"断路检测","lastcolor":"#099575","focuscolor":"#8b9398"},
				{text:"正常","subtext":"继电器保护","datatext":"继电器保护","lastcolor":"#099575","focuscolor":"#8b9398"},
				{text:"正常","subtext":"绝缘检测","datatext":"绝缘检测","lastcolor":"#099575","focuscolor":"#8b9398"},
				{text:"正常","subtext":"接地检测","datatext":"接地检测","lastcolor":"#099575","focuscolor":"#8b9398"},
				{text:"正常","subtext":"防雷检测","datatext":"防雷检测","lastcolor":"#099575","focuscolor":"#8b9398"}
			]
			getechartmark("check_left",eleftdata);
			getechartmark("check_right",erightdata);
			function getechartmark(targetId,dataArr){
				$.each(dataArr, function(i,data) {
					var dom =$("<div>");
				  	$("#"+targetId).append(dom);
					var myChart= echarts.init(dom[0]);
					var option = {
			        	title:{
							text:data.text,
			        		subtext: data.subtext,
			        		x:'center',
			        		y:'34%',
			        		textStyle:{
			            		fontSize:'18',
			            		color:'#000',
			            		fontWeight:'normal'
			            		
			            	},
			        		subtextStyle:{
			        			fontSize:'12',
			        			color:'#535353',
			        		}
			        	},
		        		tooltip:{
		        			trigger:'item',
		        			formatter: "{a} <br/>{b}"
		        		},
		        		series:[
			            	{
			            		name:data.datatext,
			            		type:'pie',
			            		radius:['70%','80%'],
			            		avoidLabelOverlap: false,
			            		label:{
			            			normal:{
			            				show:false,
			            			},
			            			emphasis:{
			            				show:false,
			            			}
			            		},
			            		itemStyle:{
			            			normal:{
			            				color: new echarts.graphic.LinearGradient(0.5, 0, 0, 0.5, [
			            					{offset: 0, color: data.lastcolor},
			            					{offset: 1, color: data.focuscolor}
		            					], false),
			            			}
			            			
			            		},
			            		data:[
			            			{value:412,name:data.text}
			            		]
			            	}
			            ]
			        };
			        // 使用刚指定的配置项和数据显示图表。
			        myChart.setOption(option);
				});
			}
